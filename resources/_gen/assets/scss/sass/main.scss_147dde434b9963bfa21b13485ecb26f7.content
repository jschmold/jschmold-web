h1,h2,h3,h4,h5,h6,span,p,button{color:#fff;margin:unset;padding:unset}a{color:#66acff}h1,.heading-1{font-size:24pt}h2,.heading-2{font-size:20pt}h3,.heading-3{font-size:18pt}h4,.heading-4{font-size:16pt}h5,.heading-5{font-size:14pt}h6,.heading-6{font-size:12pt}h1,.heading-1,h2,.heading-2,h3,.heading-3,h4,.heading-4,h5,.heading-5,h6,.heading-6{font-family:noto sans,sans-serif}p,span,button{font-family:roboto,sans-serif}p,span,button{font-size:12pt}@respond-to('medium'){h1,.heading-1{font-size:56pt}h2,.heading-2{font-size:48pt}h3,.heading-3{font-size:32pt}h4,.heading-4{font-size:24pt}h5,.heading-5{font-size:18pt}h6,.heading-6{font-size:12pt}h1,.heading-1,h2,.heading-2,h3,.heading-3,h4,.heading-4,h5,.heading-5,h6,.heading-6{font-family:'Noto Sans', sans-serif}p,span,button{font-family:'Roboto', sans-serif}p,span,button{font-size:14pt}}body{margin:unset;background-image:url(./img/background.jpg);background-repeat:no-repeat;backdrop-filter:blur(10px);background-size:cover;background-position:top;min-width:320px}body .content{min-height:100vh;width:100vw;display:flex;flex-direction:column;background-color:rgba(36,36,36,.6)}button,.button{outline:unset;border:unset;background-color:unset}button.outline,.button.outline{height:34pt;border:2pt solid #fff;border-radius:6pt}a.button-link{display:flex;align-items:center;justify-content:center;width:100%;height:100%;text-decoration:unset;color:inherit}a.button-link:active,a.button-link:visited{color:inherit}.content .go-home{padding:10pt;width:min-content;color:#fff}.content .heading{text-align:center;margin-top:15pt}@media(min-width:1000px){.content .heading{margin-top:40pt}}.content .heading .avatar{overflow:hidden;display:flex;align-items:center;justify-content:center;margin-bottom:20pt}.content .heading .avatar img{width:100pt;height:100%;border-radius:400px}@media(min-width:675px){.content .heading .avatar img{width:120pt}}@media(min-width:1000px){.content .heading .avatar img{width:180pt}}.content .subtitle{font-size:12pt}@media(min-width:1000px){.content .subtitle{font-size:initial}}.home-links{display:flex;flex-direction:column;align-items:center;margin-top:20pt}.home-links button{width:130pt;height:30pt}@media(min-width:675px){.home-links button+button{margin-top:10pt}}@media(min-width:1000px){.home-links{width:calc(156pt * 2 + (8pt * 4 * 4));margin-left:auto;margin-right:auto;justify-content:center;flex-direction:row;flex-wrap:wrap}.home-links button+button{margin-top:8pt}.home-links button{width:156pt;margin:8pt}}